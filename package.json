{
  "name": "student-spreadsheet-manager",
  "version": "1.0.0",
  "description": "An Electron app to manage student spreadsheets.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist-linux": "electron-builder --linux",
    "dist-win": "electron-builder --windows"
  },
    "build": {
    "appId": "com.kanagawa.yamada.binus.lms",
    "productName": "Binus LMS",
    "asar": true,
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "pacman"
      ],
      "category": "Education",
      "icon": "logo/LmsLogo.png"
    },
    "win": {
      "target": [
        "portable",
        "nsis"
      ],
      "icon": "logo/LmsLogo.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "logo/LmsLogo.icns"
    },

  "keywords": [],
  "author": "Wesley",
  "homepage": "https://github.com/LoggingNewMemory",
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "googleapis": "^134.0.0",
    "xlsx": "^0.18.5"
  }
  }
}
